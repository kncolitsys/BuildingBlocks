<modelglue>
	<include template="config/ModelGlue_Template.xml"/>
	<controllers>
	</controllers>
	<event-handlers>
		<event-handler name="helloworld">
			<broadcasts />
			<views>
				<include name="body" template="dsphelloworld.cfm" />
